{% xlsdocument %}
    {% xlssheet 'Test' %}
        {% xlsrow %}
            {% xlscell %}Foo{% endxlscell %}
            {% xlscell 3 %}Bar{% endxlscell %}
            {% xlscell 3 %}Lorem{% endxlscell %}
            {% xlscell %}Ipsum{% endxlscell %}
            {% xlscell 2 %}Hello{% endxlscell %}
            {% xlscell 5 %}World{% endxlscell %}
        {% endxlsrow %}
        {% xlsrow %}
            {% xlscell 1 %}Foo{% endxlscell %}
            {% xlscell 2.0 %}Bar{% endxlscell %}
            {% xlscell '3' %}Lorem{% endxlscell %}
            {% set index = 4 %}
            {% xlscell index %}Ipsum{% endxlscell %}
            {% xlscell 3 + 2 %}Hello{% endxlscell %}
            {% xlscell '6'|upper %}World{% endxlscell %}
        {% endxlsrow %}
    {% endxlssheet %}
{% endxlsdocument %}
